pipeline{
 agent any
 
  stages{
   stage('Maven build'){
    steps{
     sh "mvn clean package"
     }
   }
   stage('Docker Build Image'){
    steps{
     sh"docker build -t docker/2021myapp:v1 ."
    }
   }
   stage('push to docker hub'){
    steps{
    echo" deploy to dev environment"
    }
   }
   stage('dev-deployment'){
   steps{
    echo"deploy to uat environment"
    }
   }
  }
 } 
    
   
